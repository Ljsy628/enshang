                <view class='achievement' wx:for='{{generalize}}'  wx:key=''>
                        <view class='Introduction'>
                            <view class='introd_name'>{{item.name}}</view>
                            <view class='intop'></view>
                            <view class='inbot'>
                            <view class='blue_line'></view>
                            </view>
                        </view>
                        <view class='collab_box'>
                            <view class='collab_min' wx:for='{{generResults}}' wx:for-item='val' wx:key='' wx:if="{{val.type == item.id}}">
                                <view class='collab_left'>
                                    <image src='{{host+val.img}}'></image>
                                </view>
                                <view class='collab_right'>
                                    <view class='project_top'>
                                        <view class='project_name'>{{val.title}}</view>
                                    </view>
                                    <view class='project_brief'>{{val.subtitle}}</view>
                                </view>
                            </view> 
                        </view>
                    </view>
            </scroll-view>
        </swiper-item>
        <swiper-item>

